p = (4, 5)
x, y = p
print x, y

a = [1, "ni", "women", 1.23445]
age, sex, _, price = a
print age, sex, price

import getpass

pass = getpass.getpass()


def login(pass):
    if pass = 1:
        return 1
    else:
        return 0

if login(pass)
    print "yes"
else:
    print "no"
